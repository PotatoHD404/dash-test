{
    "builds": [
        {
            "src": "app.py",
            "use": "@potatohd/vercel-package-installer1",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.8",
                "wsgiApplicationName": "application"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}